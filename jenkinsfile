pipeline {
  agent {
      node {
          label 'NODEJS'
      }
  }
  environment {
      URL = "google.com"
  }
  stages {
      stage ('1') {
          steps {
              sh 'echo Hello World'
          }
      }
      stage ("2") {
          agent {
              node {
                  label 'JAVA'
              }
          }
          steps {
              sh 'echo Hy'
              sh 'echo ${URL}'
          }
          
      }
  }
}